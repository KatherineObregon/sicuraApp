<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MasOpciones">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="134dp"
        android:layout_height="63dp"
        android:layout_marginTop="10dp"
        android:text="Recorridos compartidos conmigo"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridosCompartidosConmigo"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridosCompartidosConmigo"
        app:layout_constraintTop_toBottomOf="@+id/btn_RecorridosCompartidosConmigo" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mis recorridos"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/btn_MisRecorridos"
        app:layout_constraintStart_toStartOf="@+id/btn_MisRecorridos"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="144dp"
        android:layout_height="41dp"
        android:layout_marginTop="10dp"
        android:text="Activar recorrido en tiempo real"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintTop_toBottomOf="@+id/btn_RecorridoTiempoReal" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compartir ruta"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/btn_CompartirRuta"
        app:layout_constraintStart_toStartOf="@+id/btn_CompartirRuta"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/btn_RecorridosCompartidosConmigo"
        android:layout_width="171dp"
        android:layout_height="154dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        android:src="@drawable/ic_recorridos_compartidos_foreground"/>

    <ImageButton
        android:id="@+id/btn_MisRecorridos"
        android:layout_width="171dp"
        android:layout_height="154dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintEnd_toEndOf="@+id/btn_CompartirRuta"
        app:layout_constraintStart_toStartOf="@+id/btn_CompartirRuta"
        app:layout_constraintTop_toTopOf="@+id/btn_RecorridoTiempoReal"
        android:src="@drawable/ic_recorridos_foreground"/>

    <ImageButton
        android:id="@+id/btn_CompartirRuta"
        android:layout_width="171dp"
        android:layout_height="154dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_contactosConfianza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_contactosConfianza"
        app:layout_constraintVertical_bias="0.0"
        android:src="@drawable/ic_compartir_ruta_foreground"/>

    <ImageButton
        android:id="@+id/btn_contactosConfianza"
        android:layout_width="171dp"
        android:layout_height="154dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_contactos_confianza_foreground"/>

    <ImageButton
        android:id="@+id/btn_RecorridoTiempoReal"
        android:layout_width="171dp"
        android:layout_height="154dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/btn_contactosConfianza"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_contactosConfianza"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:src="@drawable/ic_recorrido_real_foreground"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Contactos de confianza"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_contactosConfianza"
        app:layout_constraintStart_toStartOf="@+id/btn_contactosConfianza"
        app:layout_constraintTop_toBottomOf="@+id/btn_contactosConfianza" />

</androidx.constraintlayout.widget.ConstraintLayout>