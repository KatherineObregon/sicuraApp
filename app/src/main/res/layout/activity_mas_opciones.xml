<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MasOpciones">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="127dp"
        android:layout_height="28dp"
        android:text="Mi perfil"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/btn_MisRecorridos"
        app:layout_constraintStart_toStartOf="@+id/btn_MisRecorridos"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.571" />

    <ImageButton
        android:id="@+id/btn_MisRecorridos2"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:src="@drawable/ic_mis_recorridos2"
        app:layout_constraintBottom_toBottomOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintEnd_toEndOf="@+id/btn_CompartirRuta"
        app:layout_constraintStart_toStartOf="@+id/btn_CompartirRuta"
        app:layout_constraintTop_toTopOf="@+id/btn_RecorridoTiempoReal" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="134dp"
        android:layout_height="63dp"
        android:layout_marginTop="10dp"
        android:text="Recorridos compartidos conmigo"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridosCompartidosConmigo"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridosCompartidosConmigo"
        app:layout_constraintTop_toBottomOf="@+id/btn_RecorridosCompartidosConmigo" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mis recorridos"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/btn_MisRecorridos"
        app:layout_constraintStart_toStartOf="@+id/btn_MisRecorridos"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="144dp"
        android:layout_height="41dp"
        android:layout_marginTop="10dp"
        android:text="Activar recorrido en tiempo real"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintTop_toBottomOf="@+id/btn_RecorridoTiempoReal" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compartir ruta"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/btn_CompartirRuta"
        app:layout_constraintStart_toStartOf="@+id/btn_CompartirRuta"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/btn_RecorridosCompartidosConmigo"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/ic_recorridos_compartidos2"
        app:layout_constraintEnd_toEndOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/btn_RecorridoTiempoReal"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageButton
        android:id="@+id/btn_MisRecorridos"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:onClick="irAMiPerfil"
        android:src="@drawable/ic_mi_perfil2"
        app:layout_constraintBottom_toBottomOf="@+id/btn_RecorridosCompartidosConmigo"
        app:layout_constraintEnd_toEndOf="@+id/btn_MisRecorridos2"
        app:layout_constraintStart_toStartOf="@+id/btn_MisRecorridos2"
        app:layout_constraintTop_toTopOf="@+id/btn_RecorridosCompartidosConmigo" />

    <ImageButton
        android:id="@+id/btn_CompartirRuta"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:layout_marginEnd="50dp"
        android:src="@drawable/ic_compartir_ruta2"
        app:layout_constraintBottom_toBottomOf="@+id/btn_contactosConfianza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_contactosConfianza"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btn_contactosConfianza"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        android:onClick="irContactosConfianza"
        android:src="@drawable/ic_contactos_confianza2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_RecorridoTiempoReal"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_tiempo_real2"
        app:layout_constraintEnd_toEndOf="@+id/btn_contactosConfianza"
        app:layout_constraintHorizontal_bias="0.371"
        app:layout_constraintStart_toStartOf="@+id/btn_contactosConfianza"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Contactos de confianza"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_contactosConfianza"
        app:layout_constraintStart_toStartOf="@+id/btn_contactosConfianza"
        app:layout_constraintTop_toBottomOf="@+id/btn_contactosConfianza" />

</androidx.constraintlayout.widget.ConstraintLayout>