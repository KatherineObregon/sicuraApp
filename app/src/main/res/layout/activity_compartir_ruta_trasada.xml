<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CompartirRuta_trasada">

    <fragment
        android:id="@+id/mapa_compartir2_trasado"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="352dp"
        android:layout_height="504dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/button6_2_trasado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Encontrar ruta"
        android:theme="@style/boton_indigo"
        app:layout_constraintEnd_toEndOf="@+id/mapa_compartir2_trasado"
        app:layout_constraintStart_toStartOf="@+id/mapa_compartir2_trasado"
        app:layout_constraintTop_toBottomOf="@+id/mapa_compartir2_trasado" />

    <Button
        android:id="@+id/button7_2_trasado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Compartir ruta"
        android:theme="@style/boton_indigo"
        app:layout_constraintEnd_toEndOf="@+id/button6_2_trasado"
        app:layout_constraintStart_toStartOf="@+id/button6_2_trasado"
        app:layout_constraintTop_toBottomOf="@+id/button6_2_trasado" />

</androidx.constraintlayout.widget.ConstraintLayout>